# MindFlow Environment Configuration
# Copy this file to .env and fill in your values

# API Configuration
EXPO_PUBLIC_API_URL=https://api.mindflow.app
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# App Configuration
EXPO_PUBLIC_APP_NAME=MindFlow
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_ENVIRONMENT=development

# Feature Flags
EXPO_PUBLIC_ENABLE_AI_FEATURES=true
EXPO_PUBLIC_ENABLE_SOCIAL_FEATURES=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# External Services
EXPO_PUBLIC_WEATHER_API_KEY=your_weather_api_key_here
EXPO_PUBLIC_GOOGLE_FONTS_API_KEY=your_google_fonts_key_here

# Development
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=info

# Optional: Analytics (if enabled)
EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here
