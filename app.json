{
  "expo": {
    "name": "MindFlow",
    "slug": "mindflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindflow.app"
    },
    "android": {
      "package": "com.mindflow.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "mindflow",
    "plugins": [
      "expo-router"
    ]
  }
}
