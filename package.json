{
  "name": "mindflow",
  "version": "1.0.0",
  "description": "Your Personal Learning & Knowledge Management Platform - A modern, cross-platform app built with React Native and Expo",
  "main": "index.js",
  "keywords": [
    "learning",
    "knowledge-management",
    "education",
    "productivity",
    "react-native",
    "expo",
    "typescript",
    "cross-platform",
    "mobile-app",
    "web-app",
    "notes",
    "quiz",
    "progress-tracking"
  ],
  "author": "MindFlow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mindflow.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/mindflow/issues"
  },
  "homepage": "https://github.com/yourusername/mindflow#readme",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:web": "expo build:web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "rm -rf node_modules && yarn install",
    "reset": "rm -rf .expo && yarn start"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo-google-fonts/poppins": "^0.4.0",
    "expo": "^49.0.0",
    "expo-av": "~13.4.1",
    "expo-blur": "~12.4.1",
    "expo-document-picker": "~11.5.4",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-media-library": "~15.4.1",
    "expo-router": "^2.0.0",
    "expo-sharing": "~11.5.0",
    "expo-status-bar": "~1.6.0",
    "lucide-react-native": "^0.525.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-pdf": "^6.7.7",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "private": true
}
